NEIGHBOR=$1
SCENARIO=$2

echo $NEIGHBOR
echo $SCENARIO

PREFIX="empty-16-16-random"

KRUHOBOTS_LIST=`cat kruhobots_empty`

for KRUHOBOTS in $KRUHOBOTS_LIST;
do
    echo 'Solving '$PREFIX' scenario '$SCENARIO' MAPF-R instance with '$KRUHOBOTS' kruhobots ...'
    ../../main/mapfR_solver_main '--input-mapR-file=map_'$PREFIX'_n'$NEIGHBOR'.mapR' '--input-kruhoR-file='$PREFIX'-'$SCENARIO'_k'$KRUHOBOTS'.kruR' '--algorithm=smtcbsR*' '--output-file=solution.txt' > 'out_'$PREFIX'-'$SCENARIO'_k'$KRUHOBOTS'.txt'
done
